<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.example.lidar" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
  <name>Lidar</name>
  <js-module name="Lidar" src="www/Lidar.js">
    <clobbers target="lidar" />
  </js-module>
  <platform name="ios">
    <config-file parent="/*" target="config.xml">
      <feature name="Lidar">
        <param name="ios-package" value="Lidar" />
      </feature>
    </config-file>
    <source-file src="src/ios/Lidar.m" />
  </platform>
</plugin>
</feature>